<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.henry.wordpad">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity" />
        <activity android:name=".Education" />
        <activity android:name=".University" />
        <activity android:name=".Personal" />
        <activity android:name=".Edit" />
        <activity
            android:name=".LaunchActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ThemeLaunch">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Stock" />
        <activity android:name=".StockListActivity" />
        <activity android:name=".MyListActivity" />
        <activity android:name=".MoveList" />
        <activity android:name=".QSrank" />
        <activity android:name=".QSrank2" />
        <activity android:name=".QSrank3"></activity>
    </application>

</manifest>